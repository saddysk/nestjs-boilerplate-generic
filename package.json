{
  "name": "backend",
  "version": "0.0.1-beta.75",
  "description": "",
  "author": "1811labs",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "console:dev": "ts-node -r tsconfig-paths/register src/console.ts",
    "console": "node dist/src/console.js",
    "db:seed": "npm run console:dev db:seed",
    "dev": "nest start --watch",
    "typeorm": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js",
    "schema:sync": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js schema:sync -d ./src/database/datasource.ts",
    "schema:drop": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js schema:drop -d ./src/database/datasource.ts",
    "migration:run": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js migration:run -d ./src/database/datasource.ts",
    "migration:show": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js migration:show -d ./src/database/datasource.ts",
    "migration:generate": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js migration:generate -d ./src/database/datasource.ts",
    "migration:create": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js migration:create",
    "migration:revert": "ts-node -r tsconfig-paths/register --project tsconfig.json node_modules/typeorm/cli.js migration:revert -d ./src/database/datasource.ts",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "ci:reseed": "node -r dotenv -e \"require('dotenv').config(); process.env.APP_ENV !== 'ci' ? process.exit(11) : true;\" && npm run schema:drop && npm run migration:run && npm run console:dev -- db:seed -t",
    "fix": "run-s format lint",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "check:lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --ignore-path .gitignore --ignore-path .eslintignore",
    "format": "prettier --write .",
    "check:format": "prettier . --check --loglevel warn",
    "validate": "run-s check:**",
    "prepare": "husky install",
    "release": "bump --preid beta -c -t"
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8",
    "yarn": "use npm!"
  },
